{
  "name": "comem-travel-log-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run docs:clean && npm run docs:build && concurrently \"npm run start:dev\" \"npm run docs:serve\" \"npm run docs:watch\"",
    "docs:build": "apidoc -i api -o docs",
    "docs:clean": "rimraf docs",
    "docs:serve": "node scripts/serve-docs.js",
    "docs:watch": "onchange \"apidoc.json\" \"API.md\" \"api/index.js\" \"api/**/*.docs.js\" \"api/**/*.routes.js\" -- npm run docs:build",
    "doctoc": "doctoc --gitlab --notitle README.md",
    "postinstall": "npm run docs:build",
    "start": "node ./bin/www",
    "start:dev": "nodemon"
  },
  "dependencies": {
    "bcrypt": "^3.0.2",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "format-link-header": "^2.1.0",
    "inflection": "^1.12.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "log4js": "^3.0.6",
    "mongoose": "^5.3.15",
    "mongoose-unique-validator": "^2.0.2",
    "qs": "^6.6.0",
    "url-join": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.7",
    "concurrently": "^4.1.0",
    "doctoc": "^1.4.0",
    "get-port": "^4.0.0",
    "live-server": "^1.2.1",
    "nodemon": "^1.18.7",
    "onchange": "^5.1.3",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": "^10.14.0"
  }
}
